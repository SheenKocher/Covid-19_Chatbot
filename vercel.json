{
  "version": 2,
  "builds": [
    {
      "src": "templates/**/*.html",
      "use": "@now/static-build",
      "config": { "zeroConfig": true }
    },
    {
      "src": "static/**/*",
      "use": "@now/static"
    },
    {
      "src": "app.py",
      "use": "@now/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}
