{
  "version": 2,
  "builds": [
    {
      "src": "templates/!(index.html)",
      "use": "@now/static-build"
    },
    {
      "src": "static/**/*",
      "use": "@now/static-build"
    },
    {
      "src": "app.py",
      "use": "@now/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}
